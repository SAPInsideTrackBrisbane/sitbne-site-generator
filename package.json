{
  "name": "sitbne-sitegen",
  "description": "SITBNE site generator based on GatsbyJS starter and was themed using the Casper theme from Ghost.",
  "version": "2.0.0",
  "author": "Katan Patel",
  "dependencies": {
    "ajv": "^6.11.0",
    "babel-eslint": "^10.0.3",
    "classnames": "^2.2.6",
    "gatsby": "^2.19.12",
    "gatsby-pagination": "^1.2.0",
    "gatsby-plugin-catch-links": "^2.1.25",
    "gatsby-plugin-feed": "^2.3.27",
    "gatsby-plugin-google-analytics": "^2.1.35",
    "gatsby-plugin-lodash": "^3.1.20",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-nprogress": "^2.1.19",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-plugin-twitter": "^2.1.19",
    "gatsby-remark-autolink-headers": "^2.1.24",
    "gatsby-remark-copy-linked-files": "^2.1.37",
    "gatsby-remark-images": "^3.1.44",
    "gatsby-remark-prismjs": "^3.3.31",
    "gatsby-remark-responsive-iframe": "^2.2.32",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-json": "^2.2.26",
    "gatsby-transformer-remark": "^2.6.50",
    "latest": "^0.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prismjs": "^1.19.0",
    "react": "^16.12.0",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.12.0",
    "react-gravatar": "^2.6.3",
    "react-helmet": "^5.2.1",
    "react-scroll": "^1.7.16",
    "react-share": "^2.0.1",
    "react-social-icons": "^4.1.0",
    "react-twitter-widgets": "^1.4.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "gh-pages": "^2.2.0",
    "install-peers": "^1.0.3",
    "prettier": "^1.19.1",
    "remark-cli": "^7.0.1",
    "remark-preset-lint-recommended": "^3.0.3",
    "standard-version": "^8.0.1",
    "stylefmt": "^6.0.3",
    "stylelint": "^13.1.0",
    "stylelint-config-standard": "^20.0.0",
    "write-good": "^1.0.2"
  },
  "keywords": [
    "gatsby",
    "blog",
    "casper",
    "starter"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf .cache && rm -rf public",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:md",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public --ignore-pattern static .",
    "lint:css": "stylelint --fix 'src/**/*.css'",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "release": "standard-version -a"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
