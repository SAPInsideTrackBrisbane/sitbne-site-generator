{
  "name": "sitbne-sitegen",
  "description": "SITBNE site generator based on GatsbyJS Casper theme starter from Ghost.",
  "version": "2.0.0",
  "author": "Katan Patel",
  "dependencies": {
    "ajv": "^8.12.0",
    "babel-eslint": "^10.0.3",
    "classnames": "^2.2.6",
    "gatsby": "^5.0.0-next",
    "gatsby-pagination": "^1.2.0",
    "gatsby-plugin-catch-links": "^5.7.0",
    "gatsby-plugin-feed": "^5.7.0",
    "gatsby-plugin-google-analytics": "^5.7.0",
    "gatsby-plugin-lodash": "^6.7.0",
    "gatsby-plugin-manifest": "^5.7.0",
    "gatsby-plugin-nprogress": "^5.7.0",
    "gatsby-plugin-offline": "^6.7.0",
    "gatsby-plugin-react-helmet": "^6.7.0",
    "gatsby-plugin-sharp": "^5.7.0",
    "gatsby-plugin-sitemap": "^6.7.0",
    "gatsby-plugin-twitter": "^5.7.0",
    "gatsby-remark-autolink-headers": "^6.7.0",
    "gatsby-remark-copy-linked-files": "^6.7.0",
    "gatsby-remark-images": "^7.7.0",
    "gatsby-remark-prismjs": "^7.7.0",
    "gatsby-remark-responsive-iframe": "^6.7.0",
    "gatsby-source-filesystem": "^5.7.0",
    "gatsby-transformer-json": "^5.7.0",
    "gatsby-transformer-remark": "^6.7.0",
    "latest": "^0.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prismjs": "^1.19.0",
    "react": "^18.0.0",
    
    "react-dom": "^18.0.0",
    "react-gravatar": "^2.6.3",
    "react-helmet": "^6.1.0",
    "react-scroll": "^1.7.16",
    "react-share": "^4.4.1",
    "react-social-icons": "^5.15.0",
    "react-twitter-widgets": "^1.4.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "gh-pages": "^2.2.0",
    "install-peers": "^1.0.3",
    "prettier": "^1.19.1",
    "remark-cli": "^11.0.0",
    "remark-preset-lint-recommended": "^6.1.2",
    "standard-version": "^9.5.0",
    "stylefmt": "^6.0.3",
    "stylelint": "^15.2.0",
    "stylelint-config-standard": "^30.0.1",
    "write-good": "^1.0.2"
  },
  "keywords": [
    "gatsby",
    "blog",
    "casper",
    "starter"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "deploy:ci": "gatsby build --prefix-paths && gh-pages -d public -r https://$GH_PAGES_TOKEN@github.com/sitbne/sitbne.github.io.git",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf .cache && rm -rf public",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:md",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public --ignore-pattern static .",
    "lint:css": "stylelint --fix 'src/**/*.css'",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "release": "standard-version -a"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
